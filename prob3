
public class Main
{
	public static void main(String[] args) {
        int arr[] = {1,1,2,3,0,0,6,5,4,0,0,0,4,4};
        int count = 0;
        for(int i = 0; i <arr.length-1;i++){
            if(arr[i]=0){
                System.out.println(arr[i]);
                count++;
            }
        }
        System.out.println("count : " + count);

        }
}
